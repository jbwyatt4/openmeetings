<?xml version="1.0" encoding="UTF-8" ?>
<library>

<!-- ###################
	Class charttab
 -->
<class name="chattab" extends="simplebasetabpane" >

	<view name="_chartoutputborder" height="${ parent.height-30 }"
        bgcolor="$once{ canvas.basebgcolorizer }" width="254">
		
		<chatoutput name="_chatoutput" id="chartcontent"
					x="1" y="1"  />
		
	</view>
	
	<chattabbottom name="_chattabbottom" 
				   y="${ parent.height-28 }" x="0" width="254" />
	
</class>

</library>