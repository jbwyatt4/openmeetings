<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.xmlcrm.app.hibernate.beans.domain.Organisation_Users"
        table="organisation_users"
        lazy="false"
    >

        <id
            name="organisation_users_id"
            column="organisation_users_id"
            type="java.lang.Long"
        >
            <generator class="increment">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Organisation_Users.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="organisation"
            class="org.xmlcrm.app.hibernate.beans.domain.Organisation"
            cascade="none"
            outer-join="true"
            update="true"
            insert="true"
            column="organisation_id"
        />

        <property
            name="user_id"
            type="long"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="starttime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="starttime"
        />

        <property
            name="updatetime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="updatetime"
        />

        <property
            name="deleted"
            type="string"
            update="true"
            insert="true"
            column="deleted"
        />

        <property
            name="comment"
            type="string"
            update="true"
            insert="true"
            column="comment"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Organisation_Users.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
