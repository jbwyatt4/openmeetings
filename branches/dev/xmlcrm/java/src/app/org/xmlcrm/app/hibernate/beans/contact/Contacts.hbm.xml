<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.xmlcrm.app.hibernate.beans.contact.Contacts"
        table="contacts"
    >

        <id
            name="contact_id"
            column="contact_id"
            type="java.lang.Long"
        >
            <generator class="increment">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Contacts.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="adresses_id"
            type="long"
            update="true"
            insert="true"
            column="adresses_id"
        />

        <property
            name="age"
            type="long"
            update="true"
            insert="true"
            column="age"
        />

        <property
            name="comment"
            type="string"
            update="true"
            insert="true"
            column="comment"
        />

        <property
            name="firstname"
            type="string"
            update="true"
            insert="true"
            column="firstname"
        />

        <property
            name="freigabe_id"
            type="long"
            update="true"
            insert="true"
            column="freigabe_id"
        />

        <property
            name="lastname"
            type="string"
            update="true"
            insert="true"
            column="lastname"
        />

        <property
            name="titel_id"
            type="long"
            update="true"
            insert="true"
            column="titel_id"
        />

        <property
            name="starttime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="starttime"
        />

        <property
            name="updatetime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="updatetime"
        />

        <property
            name="deleted"
            type="boolean"
            update="true"
            insert="true"
            column="deleted"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Contacts.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
